  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm:live-server\" \"npm:tailwind-build\" \"npm:tailwind-build-more-info-css\" \"npm:tailwind-build-search-results-css\"",
    "live-server": "browser-sync start --server --files '**/*.css, **/*.html, **/*.js, !node_modules/**/*, !.git/**/*' --directory --port 7777 --browser \"C:\\Program Files\\Google\\Chrome\\Application\\chrome.exe\"",
    "tailwind-build": "tailwindcss -i src/styles.css -o public/styles.css --watch",
    "tailwind-build-more-info-css": "tailwindcss -i src/moreinfo.css -o public/moreinfo.css --watch",
    "tailwind-build-search-results-css": "tailwindcss -i src/searchresults.css -o public/searchresults.css --watch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "^2.26.13",
    "run-script-os": "^1.1.5",
    "tailwindcss": "^3.0.23"
  }
}
